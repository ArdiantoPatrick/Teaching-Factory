<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">
    <link href="~/Service/img/logo/logo.png" rel="icon">
    <title>Teaching Factory</title>
    <link href="~/Service/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
    <link href="~/Service/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet" type="text/css">
    <link href="~/Service/css/ruang-admin.min.css" rel="stylesheet">
    <script src="~/Service/vendor/jquery/jquery.min.js"></script>
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.13.1/css/jquery.dataTables.min.css">
    <link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
    <script type="text/javascript" charset="utf8" src="https://cdn.datatables.net/1.13.1/js/jquery.dataTables.min.js"></script>
    <script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
    <script src="//cdn.jsdelivr.net/npm/sweetalert2@10/dist/sweetalert2.min.js"></script>
    <link rel="stylesheet" href="//cdn.jsdelivr.net/npm/sweetalert2@10/dist/sweetalert2.min.css" id="theme-styles">
    <link href="~/fontawesomev5/css/fontawesome.css" rel="stylesheet" />
    <link href="~/fontawesomev5/css/fontawesome.min.css" rel="stylesheet" />
</head>

<body id="page-top">
    <div id="wrapper">
        <!-- Sidebar -->
        <ul class="navbar-nav sidebar sidebar-light accordion" id="accordionSidebar">
            <a class="sidebar-brand d-flex align-items-center justify-content-center" asp-controller="Dashboard" asp-action="ServiceAdvisor">
                <div class="sidebar-brand-icon">
                    <img src="~/Service/img/logo/logo.png">
                </div>
                <div class="sidebar-brand-text mx-3">Teaching Factory</div>
            </a>
            <hr class="sidebar-divider my-0">
            <li class="nav-item">
                <a class="nav-link active" asp-controller="Dashboard" asp-action="ServiceAdvisor">
                    <i class="fas fa-fw fa-tachometer-alt fa-sm fa-fw mr-2 text-dark"></i>
                    <span>Dashboard</span>
                </a>
            </li>
            <hr class="sidebar-divider">
            <div class="sidebar-heading">
                Features
            </div>
            <li class="nav-item">
                <a class="nav-link collapsed" data-toggle="collapse" data-target="#collapseForm" aria-expanded="true" aria-controls="collapseForm">
                    <i class="fa fa-cloud icon-lg icon-cyan icon-bg-cyan icon-bg-circle fa-sm fa-fw mr-2 text-dark"></i>
                    <span>Service</span>
                </a>
                <div id="collapseForm" class="collapse" aria-labelledby="headingForm" data-parent="#accordionSidebar">
                    <div class="bg-white py-2 collapse-inner rounded">
                        <h6 class="collapse-header">Service</h6>
                        <a class="collapse-item" asp-controller="Booking" asp-action="Booking"><i class="fas fa-list fa-sm fa-fw mr-2 text-dark"></i>&nbsp;&nbsp; Booking</a>
                        <a class="collapse-item" asp-controller="RoundChecking" asp-action="RoundChecking"><i class="fas fa-chalkboard fa-sm fa-fw mr-2 text-dark"></i>&nbsp;&nbsp; Round Checking</a>
                        <a class="collapse-item" asp-controller="PKB" asp-action="PKB"><i class="fas fa-search-plus fa-sm fa-fw mr-2 text-dark"></i>&nbsp;&nbsp; Transaksi PKB</a>
                        <a class="collapse-item" asp-controller="PKB" asp-action="PKBSelesai"><i class="fas fa-check-double fa-sm fa-fw mr-2 text-dark"></i>&nbsp;&nbsp;  Konfirmasi</a>
                    </div>
                </div>
            </li>
            <li class="nav-item">
                <a class="nav-link collapsed" data-toggle="collapse" data-target="#collapseBootstrap" aria-expanded="true" aria-controls="collapseBootstrap">
                    <i class="far fa-fw fa-window-maximize fa-sm fa-fw mr-2 text-dark"></i>
                    <span>Data</span>
                </a>
                <div id="collapseBootstrap" class="collapse" aria-labelledby="headingBootstrap" data-parent="#accordionSidebar">
                    <div class="bg-white py-2 collapse-inner rounded">
                        <h6 class="collapse-header">List Data</h6>
                        <a class="collapse-item" asp-controller="MasterTempatKerja" asp-action="Index"><i class="fas fa-home fa-sm fa-fw mr-2 text-dark"></i>&nbsp;&nbsp; Data Tempat Kerja</a>
                        <a class="collapse-item" asp-controller="MasterMekanik" asp-action="Index"><i class="fas fa-users-cog fa-sm fa-fw mr-2 text-dark"></i>&nbsp;&nbsp; Data Mekanik</a>
                        <a class="collapse-item" asp-controller="MasterCustomer" asp-action="Index"><i class="fas fa-user fa-sm fa-fw mr-2 text-dark"></i>&nbsp;&nbsp; Data Customer</a>
                        <a class="collapse-item" asp-controller="MasterGrup" asp-action="Index"><i class="fas fa-users fa-sm fa-fw mr-2 text-dark"></i>&nbsp;&nbsp; Data Grup</a>
                    </div>
                </div>
            </li>
            <li class="nav-item">
                <a class="nav-link" asp-controller="RiwayatTransaksi" asp-action="RiwayatTransaksi">
                    <i class="fas fa-list fa-sm fa-fw mr-2 text-dark"></i>
                    <span>Riwayat Transaksi</span>
                </a>
            </li>
            <hr class="sidebar-divider">
            <div class="sidebar-heading">
                Account
            </div>
            <li class="nav-item">
                <a class="nav-link" asp-controller="Profile" asp-action="ProfileSA">
                    <i class="fas fa-user fa-sm fa-fw mr-2 text-dark"></i>
                    <span>Profile</span>
                </a>
            </li>
            <li class="nav-item">
                <a class="nav-link" asp-controller="Home" asp-action="Logout">
                    <i class="fas fa-sign-out-alt fa-sm fa-fw mr-2 text-dark"></i>
                    <span>Logout</span>
                </a>
            </li>
        </ul>
        <!-- Sidebar -->
        <div id="content-wrapper" class="d-flex flex-column">
            <div id="content">
                <!-- TopBar -->
                <nav class="navbar navbar-expand navbar-light bg-navbar topbar mb-4 static-top">
                    <button id="sidebarToggleTop" class="btn btn-link rounded-circle mr-3">
                        <i class="fa fa-bars"></i>
                    </button>
                    <ul class="navbar-nav ml-auto">
                        <li class="nav-item dropdown no-arrow">
                            <a class="nav-link dropdown-toggle" id="userDropdown" role="button" data-toggle="dropdown"
                               aria-haspopup="true" aria-expanded="false">
@*                                <img class="img-profile rounded-circle" src="~/Service/img/boy.png" style="max-width: 60px">*@
                                <i class="fa fa-user" aria-hidden="true"></i> <span class="ml-2 d-none d-lg-inline text-white medium resposive">@ViewBag.namasa (Service Advisor)</span>
                            </a>
                        </li>
                    </ul>
                </nav>
                <!-- Topbar -->
                <!-- Container Fluid-->
                <div class="container-fluid" id="container-wrapper">
                    @RenderBody()
                </div>
            </div>
            <!-- Footer -->
            <footer class="sticky-footer bg-white">
                <div class="container my-auto">
                    <div class="copyright text-center my-auto">
                        <span>
                            copyright &copy; <script>document.write(new Date().getFullYear());</script> - developed by
                            <b><a href="https://indrijunanda.gitlab.io/" target="_blank">Kelompok 1</a></b>
                        </span>
                    </div>
                </div>
            </footer>
            <!-- Footer -->
        </div>
    </div>

    <!-- Scroll to top -->
    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fas fa-angle-up"></i>
    </a>

    <script src="~/Service/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/Service/vendor/jquery-easing/jquery.easing.min.js"></script>
    <script src="~/Service/js/ruang-admin.min.js"></script>
    <script src="~/Service/js/demo/chart-area-demo.js"></script>
    <script src="~/website/assets/js/site.js"></script>
    @if (TempData["Notifikasi"] != null)
    {
        <script type="text/javascript">
            window.onload = function () {
                Swal.fire(
                    'Berhasil !',
                    '@TempData["Notifikasi"]',
                    'success'
                )
            }
        </script>
    }

    @if (TempData["Warning"] != null)
    {
        <script type="text/javascript">
            window.onload = function () {
                Swal.fire(
                    'Peringatan !',
                    '@TempData["Warning"]',
                    'error'
                )
            }
        </script>
    }

    <script>
        $("a").on("click", function () {
            $("a").removeClass("active");
            $(this).addClass("active");
        });
    </script>
</body>
</html>