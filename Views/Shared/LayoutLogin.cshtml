@if (TempData["Notifikasi"] != null)
{
    <script type="text/javascript">
        window.onload = function () {
            Swal.fire(
                'Berhasil !',
                '@TempData["Notifikasi"]',
                'success'
            )
        }
    </script>
}

@if (TempData["Warning"] != null)
{
    <script type="text/javascript">
        window.onload = function () {
            Swal.fire(
                'Peringatan !',
                '@TempData["Warning"]',
                'error'
            )
        }
    </script>
}