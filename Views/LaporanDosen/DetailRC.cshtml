@model ProjekTeFa.Models.RoundChecking
@{
    Layout = "~/Views/Shared/LayoutDosenInstruktur.cshtml";
}
<div class="d-sm-flex align-items-center justify-content-between mb-4">
    <h1 class="h3 mb-0 text-gray-800">Detail Round Checking</h1>
    <a asp-controller="LaporanDosen" asp-action="RiwayatTransaksi">
        <button type="button" class="btn btn-outline-primary mb-4" style="width: 150px">Back</button>
    </a>
</div>

<form role="form" method="post">
    <div class="row">
        <div class="col-lg-6">
            <!-- Form Basic -->
            <div class="card mb-4">
                <div class="card-header py-3 d-flex flex-row align-items-center justify-content-between">
                    <h6 class="m-0 font-weight-bold text-warning">CUSTOMER AND VEHICLE INFORMATION</h6>
                </div>
                <div class="card-body">
                    <div class="row">
                        <div class="form-group row col-6">
                            <label for="date" class="col-sm-4 col-form-label">Date :</label>
                            <div class="col-sm-8">
                                <input type="hidden" class="form-control" id="id" asp-for="id_booking" value="@ViewBag.data.id_booking">
                                <input type="text" class="form-control" id="date" asp-for="tanggal" value="@ViewBag.datarc.tanggal" readonly>
                            </div>
                        </div>
                        <div class="form-group row col-6">
                            <label for="time" class="col-sm-4 col-form-label">Time In :</label>
                            <div class="col-sm-8">
                                <input type="text" class="form-control" id="time" asp-for="waktu" value="@ViewBag.datarc.waktu" readonly>
                            </div>
                        </div>
                    </div>
                    <div class=" form-group row">
                        <label for="nama" class="col-sm-4 col-form-label">Customer Name :</label>
                        <div class="col-sm-8">
                            <input type="text" class="form-control" id="nama" name="nama" value="@ViewBag.data.Customer.nama_customer" readonly>
                        </div>
                    </div>
                    <div class=" form-group row">
                        <label for="alamat" class="col-sm-4 col-form-label">Address :</label>
                        <div class="col-sm-8">
                            <textarea type="text" rows="4" class="form-control" id="alamat" name="alamat" readonly>@ViewBag.data.Customer.alamat</textarea>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="telepon" class="col-sm-4 col-form-label">Today's Contact Phone :</label>
                        <div class="col-sm-8">
                            <input type="number" class="form-control" id="telepon" name="telepon" value="@ViewBag.data.Customer.notelp_customer" readonly>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="jenis" class="col-sm-4 col-form-label">Vehicle Type :</label>
                        <div class="col-sm-8">
                            <input type="text" class="form-control" id="jenis" name="jenis" value="@ViewBag.data.jenis_kendaraan" readonly>
                        </div>
                    </div>
                    <div class="row">
                        <div class="form-group row col-6">
                            <label for="merk" class="col-sm-4 col-form-label">Model :</label>
                            <div class="col-sm-8">
                                <input type="text" class="form-control" id="merk" name="merk" value="@ViewBag.data.merk_kendaraan" readonly>
                            </div>
                        </div>
                        <div class="form-group row col-6">
                            <label for="tahun" class="col-sm-4 col-form-label">Year :</label>
                            <div class="col-sm-8">
                                <input type="text" class="form-control" id="tahun" name="tahun" value="@ViewBag.data.tahun_pembuatan" readonly>
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div class="form-group row col-6">
                            <label for="odometer" class="col-sm-4 col-form-label">Mileage :</label>
                            <div class="col-sm-8">
                                <input type="text" class="form-control" id="odometer" name="odometer" value="@ViewBag.data.odometer" readonly>
                            </div>
                        </div>
                        <div class="form-group row col-6">
                            <label for="plat" class="col-sm-4 col-form-label">License :</label>
                            <div class="col-sm-8">
                                <input type="text" class="form-control" id="plat" name="plat" value="@ViewBag.data.nomor_polisi" readonly>
                            </div>
                        </div>
                    </div>

                    <div class="form-group row">
                        <label for="vin" class="col-sm-4 col-form-label">VIN :</label>
                        <div class="col-sm-8">
                            <input type="text" class="form-control" id="vin" asp-for="vin" value="@ViewBag.datarc.vin" readonly>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="col-lg-6">
            <!-- General Element -->
            <div class="card mb-4">
                <div class="card-header">
                    <h6 class="m-0 font-weight-bold text-warning">SERVICE INFORMATION</h6>
                </div>
                <div class="card-body">
                    <div class="form-group">
                        <label for="sa" class="form-label">Service Advisor In Charge :</label>
                        <input type="text" class="form-control" id="sa" value="@ViewBag.datarc.nama_sa" asp-for="nama_sa" readonly>
                    </div>
                    <div class="form-group">
                        <label>Description of service to be performed :</label>
                        <textarea rows="4" class="form-control" id="perbaikan" name="perbaikan" readonly>@ViewBag.datarc.deskripsi_service</textarea>
                    </div>
                    <div class="form-group row">
                        <label class="form-label col-sm-3">Initial Estimate :</label>
                        <div class="col-sm-9">
                            <input type="number" class="form-control" id="perkiraanharga" value="@ViewBag.datarc.perkiraan_biaya" asp-for="perkiraan_biaya" readonly>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="form-label text-justify">Saya dengan ini mengizinkan perbaikan dan pemeriksaan yang dijelaskan untuk dilakukan bersamaan dengan bahan yang diperlukan dan setuju bahwa dealer tidak bertanggung jawab atas kehilangan atau kerusakan kendaraan atau barang yang tertinggal di dalam kendaraan jika terjadi kebakaran, pencurian atau sebab apa pun di luar kendali mereka. Saya dengan ini memberi Anda dan/atau karyawan Anda izin untuk mengoperasikan kendaraan sebagaimana diperlukan untuk tujuan pengujian dan inspeksi</label>
                    </div>
                    <div class="custom-control custom-checkbox">
                        @if (ViewBag.datarc.declines_sparetire == "Yes")
                        {
                            <input type="checkbox" class="custom-control-input col-sm-4" name="declines_sparetire" id="sparetire" value="Yes" asp-for="declines_sparetire" checked disabled />
                        }
                        else
                        {
                            <input type="checkbox" class="custom-control-input col-sm-4" name="declines_sparetire" id="sparetire" value="Yes" asp-for="declines_sparetire" disabled />
                        }
                        <label class="custom-control-label" for="sparetire">Customer declines access to spare tire</label>
                    </div>
                    <div class="custom-control custom-checkbox">
                        @if (ViewBag.datarc.declines_glovebox == "Yes")
                        {
                            <input type="checkbox" class="custom-control-input" name="declines_glovebox" id="glovebox" value="Yes" asp-for="declines_glovebox" checked disabled />
                        }
                        else
                        {
                            <input type="checkbox" class="custom-control-input" name="declines_glovebox" id="glovebox" value="Yes" asp-for="declines_glovebox" disabled />
                        }
                        <label class="custom-control-label" for="glovebox">Customer declines access to glove box</label>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-lg-12 mb-3" style="padding-left:250px">
            <div class="row">
                <div class="custom-control align-middle custom-radio ml-3" style="background-color:#117608;">
                    <input type="radio" id="contoh_good" name="contoh" value="Good" class=" ml-1 custom-control-input" disabled>
                    <label class="custom-control-label ml-2 mt-1" for="contoh_good"></label>
                </div>
                <label class="mr-3 ml-2 mt-1">Checked and OK at this time</label>
                <div class="custom-control custom-radio" style="background-color:#F6F300">
                    <input type="radio" id="contoh_warning" name="contoh" value="Warning" class="custom-control-input" disabled>
                    <label class="custom-control-label ml-2 mt-1" for="contohv_warning"></label>
                </div>
                <label class="mr-3 ml-2 mt-1">May require future attention</label>
                <div class="custom-control custom-radio" style="background-color:#ED1818">
                    <input type="radio" id="contoh_danger" name="contoh" value="Danger" class="custom-control-input" disabled>
                    <label class="custom-control-label ml-2 mt-1" for="contoh_danger"></label>
                </div>
                <label class="mr-3 ml-2 mt-1">Requires immediate attention</label>
            </div>
        </div>
        <div class="col-lg-6">
            <!-- Form Sizing -->
            <div class="card mb-3">
                <div class="card-header py-3 d-flex flex-row align-items-center justify-content-between">
                    <h6 class="m-0 font-weight-bold text-primary">EXTERIOR</h6>
                </div>
                <div class="card-body">
                    <div class="row">
                        <div class="custom-control custom-radio ml-3" style="background-color:#117608;">
                            @if (ViewBag.datarc.e_horn_operation == "Good")
                            {
                                <input type="radio" id="e_horn_good" value="Good" class="custom-control-input" asp-for="e_horn_operation" checked disabled>
                            }
                            else
                            {
                                <input type="radio" id="e_horn_good" value="Good" class="custom-control-input" asp-for="e_horn_operation" disabled>
                            }
                            <label class="custom-control-label ml-2 mt-1" for="e_horn_good"></label>
                        </div>
                        <div class="custom-control custom-radio" style="background-color:#F6F300 ; padding-right:8px">
                            @if (ViewBag.datarc.e_horn_operation == "Warning")
                            {
                                <input type="radio" id="e_horn_warning" value="Warning" class="custom-control-input" asp-for="e_horn_operation" checked disabled>
                            }
                            else
                            {
                                <input type="radio" id="e_horn_warning" value="Warning" class="custom-control-input" asp-for="e_horn_operation" disabled>
                            }
                            <label class="custom-control-label ml-2 mt-1" for="e_horn_warning" hidden></label>
                        </div>
                        <div class="custom-control custom-radio" style="background-color:#ED1818">
                            @if (ViewBag.datarc.e_horn_operation == "Danger")
                            {
                                <input type="radio" id="e_horn_danger" value="Danger" class="custom-control-input" asp-for="e_horn_operation" checked disabled>
                            }
                            else
                            {
                                <input type="radio" id="e_horn_danger" value="Danger" class="custom-control-input" asp-for="e_horn_operation" disabled>
                            }
                            <label class="custom-control-label ml-2 mt-1" for="e_horn_danger"></label>
                        </div>
                        <legend class="col-form-label col-sm-9 pt-0 mt-1 mt-1">Horn Operation</legend>
                    </div>
                    <div class="row">
                        <div class="custom-control custom-radio ml-3" style="background-color:#117608">
                            @if (ViewBag.datarc.e_lights == "Good")
                            {
                                <input type="radio" id="e_head_good" value="Good" class="custom-control-input" asp-for="e_lights" checked disabled>
                            }
                            else
                            {
                                <input type="radio" id="e_head_good" value="Good" class="custom-control-input" asp-for="e_lights" disabled>
                            }
                            <label class="custom-control-label ml-2 mt-1" for="e_head_good"></label>
                        </div>
                        <div class="custom-control custom-radio" style="background-color:#F6F300; padding-right:8px">
                            @if (ViewBag.datarc.e_lights == "Warning")
                            {
                                <input type="radio" id="e_head_warning" value="Warning" class="custom-control-input" asp-for="e_lights" checked disabled>
                            }
                            else
                            {
                                <input type="radio" id="e_head_warning" value="Warning" class="custom-control-input" asp-for="e_lights" disabled>
                            }
                            <label class="custom-control-label" for="e_head_warning" hidden></label>
                        </div>
                        <div class="custom-control custom-radio" style="background-color:#ED1818">
                            @if (ViewBag.datarc.e_lights == "Danger")
                            {
                                <input type="radio" id="e_head_danger" value="Danger" class="custom-control-input" asp-for="e_lights" checked disabled>
                            }
                            else
                            {
                                <input type="radio" id="e_head_danger" value="Danger" class="custom-control-input" asp-for="e_lights" disabled>
                            }
                            <label class="custom-control-label ml-2 mt-1" for="e_head_danger"></label>
                        </div>
                        <legend class="col-form-label col-sm-9 pt-0 mt-1 mt-1">Head lights/ tail lights/ turn signals / brake lights / hazard warning lights / exterior lamps</legend>
                    </div>
                    <div class="row">
                        <div class="custom-control custom-radio ml-3" style="background-color:#117608">
                            @if (ViewBag.datarc.e_washer_operation == "Good")
                            {
                                <input type="radio" id="e_wiper_good" value="Good" class="custom-control-input" asp-for="e_washer_operation" checked disabled>
                            }
                            else
                            {
                                <input type="radio" id="e_wiper_good" value="Good" class="custom-control-input" asp-for="e_washer_operation" disabled>
                            }
                            <label class="custom-control-label ml-2 mt-1" for="e_wiper_good"></label>
                        </div>
                        <div class="custom-control custom-radio" style="background-color:#F6F300 ; padding-right:8px">
                            @if (ViewBag.datarc.e_washer_operation == "Warning")
                            {
                                <input type="radio" id="e_wiper_warning" value="Warning" class="custom-control-input" asp-for="e_washer_operation" checked disabled>
                            }
                            else
                            {
                                <input type="radio" id="e_wiper_warning" value="Warning" class="custom-control-input" asp-for="e_washer_operation" disabled>
                            }
                            <label class="custom-control-label ml-2 mt-1" for="e_wiper_warning" hidden></label>
                        </div>
                        <div class="custom-control custom-radio" style="background-color:#ED1818">
                            @if (ViewBag.datarc.e_washer_operation == "Danger")
                            {
                                <input type="radio" id="e_wiper_danger" value="Danger" class="custom-control-input" asp-for="e_washer_operation" checked disabled>
                            }
                            else
                            {
                                <input type="radio" id="e_wiper_danger" value="Danger" class="custom-control-input" asp-for="e_washer_operation" disabled>
                            }
                            <label class="custom-control-label ml-2 mt-1" for="e_wiper_danger"></label>
                        </div>
                        <legend class="col-form-label col-sm-8 pt-0 mt-1">Windshield wiper and washer operation</legend>

                    </div>
                    <div class="row">
                        <div class="custom-control custom-radio  ml-3" style="background-color:#117608">
                            @if (ViewBag.datarc.e_winshield == "Good")
                            {
                                <input type="radio" id="e_glass_good" value="Good" class="custom-control-input" asp-for="e_winshield" checked disabled>
                            }
                            else
                            {
                                <input type="radio" id="e_glass_good" value="Good" class="custom-control-input" asp-for="e_winshield" disabled>
                            }
                            <label class="custom-control-label ml-2 mt-1" for="e_glass_good"></label>
                        </div>
                        <div class="custom-control custom-radio" style="background-color:#F6F300; padding-right:8px">
                            @if (ViewBag.datarc.e_winshield == "Warning")
                            {
                                <input type="radio" id="e_glass_warning" value="Warning" class="custom-control-input" asp-for="e_winshield" checked disabled>
                            }
                            else
                            {
                                <input type="radio" id="e_glass_warning" value="Warning" class="custom-control-input" asp-for="e_winshield" disabled>
                            }
                            <label class="custom-control-label ml-2 mt-1" for="e_glass_warning" hidden></label>
                        </div>
                        <div class="custom-control custom-radio" style="background-color:#ED1818">
                            @if (ViewBag.datarc.e_winshield == "Danger")
                            {
                                <input type="radio" id="e_glass_danger" value="Danger" class="custom-control-input" asp-for="e_winshield" checked disabled>
                            }
                            else
                            {
                                <input type="radio" id="e_glass_danger" value="Danger" class="custom-control-input" asp-for="e_winshield" disabled>
                            }
                            <label class="custom-control-label ml-2 mt-1" for="e_glass_danger"></label>
                        </div>
                        <legend class="col-form-label col-sm-8 pt-0 mt-1">Windshield glass</legend>
                    </div>
                    <div class="row">
                        <div class="custom-control custom-radio ml-3" style="background-color:#117608">
                            @if (ViewBag.datarc.e_fuel_tank_cap == "Good")
                            {
                                <input type="radio" id="e_tank_good" value="Good" class="custom-control-input" asp-for="e_fuel_tank_cap" checked disabled>
                            }
                            else
                            {
                                <input type="radio" id="e_tank_good" value="Good" class="custom-control-input" asp-for="e_fuel_tank_cap" disabled>
                            }
                            <label class="custom-control-label ml-2 mt-1" for="e_tank_good"></label>
                        </div>
                        <div class="custom-control custom-radio" style="background-color:#F6F300; padding-right:8px">
                            @if (ViewBag.datarc.e_fuel_tank_cap == "Warning")
                            {
                                <input type="radio" id="e_tank_warning" value="Warning" class="custom-control-input" asp-for="e_fuel_tank_cap" checked disabled>
                            }
                            else
                            {
                                <input type="radio" id="e_tank_warning" value="Warning" class="custom-control-input" asp-for="e_fuel_tank_cap" disabled>
                            }
                            <label class="custom-control-label ml-2 mt-1" for="e_tank_warning" hidden></label>
                        </div>
                        <div class="custom-control custom-radio" style="background-color:#ED1818">
                            @if (ViewBag.datarc.e_fuel_tank_cap == "Danger")
                            {
                                <input type="radio" id="e_tank_danger" value="Danger" class="custom-control-input" asp-for="e_fuel_tank_cap" checked disabled>
                            }
                            else
                            {
                                <input type="radio" id="e_tank_danger" value="Danger" class="custom-control-input" asp-for="e_fuel_tank_cap" disabled>
                            }
                            <label class="custom-control-label ml-2 mt-1" for="e_tank_danger"></label>
                        </div>
                        <legend class="col-form-label col-sm-8 pt-0 mt-1">Fuel tank cap gasket</legend>
                    </div>
                </div>
            </div>

            <div class="card mb-3">
                <div class="card-header py-3 d-flex flex-row align-items-center justify-content-between">
                    <h6 class="m-0 font-weight-bold text-primary">INTERIOR</h6>
                </div>
                <div class="card-body">
                    <div class="row">
                        <div class="custom-control custom-radio ml-3" style="background-color:#117608">
                            @if (ViewBag.datarc.i_combination_meter == "Good")
                            {
                                <input type="radio" id="i_dome_good" value="Good" class="custom-control-input" asp-for="i_combination_meter" checked disabled>
                            }
                            else
                            {
                                <input type="radio" id="i_dome_good" value="Good" class="custom-control-input" asp-for="i_combination_meter" disabled>
                            }
                            <label class="custom-control-label ml-2 mt-1" for="i_dome_good"></label>
                        </div>
                        <div class="custom-control custom-radio" style="background-color:#F6F300; padding-right:8px" asp-for="i_combination_meter" disabled>
                            <input type="radio" id="i_dome_warning" value="Warning" class="custom-control-input">
                            <label class="custom-control-label ml-2 mt-1" for="i_dome_warning" hidden></label>
                        </div>
                        <div class="custom-control custom-radio" style="background-color:#ED1818">
                            @if (ViewBag.datarc.i_combination_meter == "Danger")
                            {
                                <input type="radio" id="i_dome_danger" value="Danger" class="custom-control-input" asp-for="i_combination_meter" checked disabled>
                            }
                            else
                            {
                                <input type="radio" id="i_dome_danger" value="Danger" class="custom-control-input" asp-for="i_combination_meter" disabled>
                            }
                            <label class="custom-control-label ml-2 mt-1" for="i_dome_danger"></label>
                        </div>
                        <legend class="col-form-label col-sm-8 pt-0">Dome light / amp light / dimmer combination meter</legend>
                    </div>
                    <div class="row">
                        <div class="custom-control custom-radio ml-3" style="background-color:#117608">
                            @if (ViewBag.datarc.i_cabin_air_filter == "Good")
                            {
                                <input type="radio" id="i_cabin_good" value="Good" class="custom-control-input" asp-for="i_cabin_air_filter" checked disabled>
                            }
                            else
                            {
                                <input type="radio" id="i_cabin_good" value="Good" class="custom-control-input" asp-for="i_cabin_air_filter" disabled>
                            }
                            <label class="custom-control-label ml-2 mt-1" for="i_cabin_good"></label>
                        </div>
                        <div class="custom-control custom-radio" style="background-color:#F6F300">
                            @if (ViewBag.datarc.i_cabin_air_filter == "Warning")
                            {
                                <input type="radio" id="i_cabin_warning" value="Warning" class="custom-control-input" asp-for="i_cabin_air_filter" checked disabled>
                            }
                            else
                            {
                                <input type="radio" id="i_cabin_warning" value="Warning" class="custom-control-input" asp-for="i_cabin_air_filter" disabled>
                            }
                            <label class="custom-control-label ml-2 mt-1" for="i_cabin_warning"></label>
                        </div>
                        <div class="custom-control custom-radio" style="background-color:#ED1818">
                            @if (ViewBag.datarc.i_cabin_air_filter == "Danger")
                            {
                                <input type="radio" id="i_cabin_danger" value="Danger" class="custom-control-input" asp-for="i_cabin_air_filter" checked disabled>
                            }
                            else
                            {
                                <input type="radio" id="i_cabin_danger" value="Danger" class="custom-control-input" asp-for="i_cabin_air_filter" disabled>
                            }
                            <label class="custom-control-label ml-2 mt-1" for="i_cabin_danger"></label>
                        </div>
                        <legend class="col-form-label col-sm-8 pt-0">Cabin air filter</legend>
                    </div>
                    <div class="row">
                        <div class="custom-control custom-radio ml-3" style="background-color:#117608">
                            @if (ViewBag.datarc.i_parking_brake == "Good")
                            {
                                <input type="radio" id="i_parking_good" value="Good" class="custom-control-input" asp-for="i_parking_brake" checked disabled>
                            }
                            else
                            {
                                <input type="radio" id="i_parking_good" value="Good" class="custom-control-input" asp-for="i_parking_brake" disabled>
                            }
                            <label class="custom-control-label ml-2 mt-1" for="i_parking_good"></label>
                        </div>
                        <div class="custom-control custom-radio" style="background-color:#F6F300 ; padding-right:8px">
                            <input type="radio" id="i_parking_warning" value="Warning" class="custom-control-input" asp-for="i_parking_brake" disabled>
                            <label class="custom-control-label ml-2 mt-1" for="i_parking_warning" hidden></label>
                        </div>
                        <div class="custom-control custom-radio" style="background-color:#ED1818">
                            @if (ViewBag.datarc.i_parking_brake == "Danger")
                            {
                                <input type="radio" id="i_parking_danger" value="Danger" class="custom-control-input" asp-for="i_parking_brake" checked disabled>
                            }
